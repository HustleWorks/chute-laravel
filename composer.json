{
  "name": "hustleworks/chute-laravel",
  "description": "Image upload management.",
  "license": "MIT",
  "authors": [
    {
      "name": "Don Herre",
      "email": "don@hustleworks.com",
      "homepage": "https://hustleworks.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "hustleworks/chute": "dev-master",
    "illuminate/support": "^5.0,<5.6",
    "illuminate/console": "^5.0,<5.6",
    "illuminate/filesystem": "^5.0,<5.6",
    "ramsey/uuid": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "HustleWorks\\ChuteLaravel\\": "src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@git.hustle.works:internal-projects/chute.git"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HustleWorks\\ChuteLaravel\\ChuteServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
